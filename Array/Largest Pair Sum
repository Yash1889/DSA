//C++
//GFG
//TIME COMPLEXITY :- O(nlogn)
//SPACE COMPLEXITY :- O(1)


class Solution {
  public:
    int pairsum(vector<int> &arr) {
        // code here
        int t;
        int n = arr.size();
        sort(begin(arr),end(arr));
        for(int i=0;i<n;i++){
            t = arr[n-1]+arr[n-2];
        }
        return t;
    }
};
